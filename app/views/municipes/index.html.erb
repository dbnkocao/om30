<div class="container">
  <div class="card">
    <div class="card-content">
      <span class="card-title activator grey-text text-darken-4"><%= t('page.titles.municipes')%></span>
      <div class="row">
        <div class="col s6">
          <a class="waves-effect waves-light btn" id="create-municipe-button"><%= t('helpers.submit.create') %></a>
        </div>

        <div class="col s6">
          <%= text_field_tag :query, nil, class: "col s2 right", placeholder: "Pesquisar" %>
        </div>
      </div>

      <div id="municipe-list" class="municipe-list">
        <%= render "municipes/list" %>
      </div>
      <br>
    </div>
  </div>
</div>